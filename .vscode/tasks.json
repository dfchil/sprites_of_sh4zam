{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "process",
			"command": "bash",
			"args": [
				"--init-file",
				"${workspaceFolder}/.vscode/kos_make.sh"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
		{
			"label": "Cleanup",
			"type": "shell",
			"command": "pkill -9 gdb",
			"group": {
				"kind": "none",
				"isDefault": true
			},
		},
	]
}